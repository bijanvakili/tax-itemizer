{
  "DATABASE_URI": "postgresql://localhost:5432/receipts_dev",
  "DEBUG": true,
  "EXCLUSION_FILTER_MODULES": [
    "taxes.receipts.filters",
    "taxes.discrete.filters"
  ],
  "LOGGING": {
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "default": {
            "format": "%(levelname)s %(message)s"
        }
    },
    "handlers": {
        "console": {
            "level": "INFO",
            "class": "logging.StreamHandler",
            "formatter": "default"
        }
    },
    "loggers": {
        "taxes.receipts": {
            "level": "DEBUG",
            "handlers": ["console"],
            "formatter": "default"
        },
        "django.db.backends": {
            "level": "DEBUG",
            "handlers": ["console"],
            "formatter": "default"
        }
    }
  }
}
